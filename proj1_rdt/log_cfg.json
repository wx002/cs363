{
      "version": 1,
      "formatters": {
          "detailed": {
              "format": "%(asctime)s %(name)-20s %(levelname)-8s %(message)s"
          },
          "simple": {
              "format": "%(levelname)s %(message)s"
          }
      },
      "handlers": {
          "console": {
              "class": "logging.StreamHandler",
              "formatter": "detailed"
          }
      },
      "loggers": {
          "algs": {
              "level": "DEBUG"
          },
          "algs.udp_wrapper": {
              "level": "WARN"
          }
      },
      "root": {
          "handlers": ["console"],
          "level": "DEBUG"
      }
  }
